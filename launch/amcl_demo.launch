<?xml version="1.0"?>

<launch>

  <!-- Run the map server -->
  <arg name="map_file" default="$(find aiibot_navigation)/maps/aiiage_map.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

  <!--- Run AMCL -->
  <include file="$(find aiibot_navigation)/launch/amcl.launch" />

  <!--- Run Move Base -->
  <include file="$(find aiibot_navigation)/launch/move_base.launch">
    <arg name="no_map" value="false"/>
  </include>
  
</launch>
